    "assets-webpack-plugin": "^3.9.7",
    "autodll-webpack-plugin": "^0.4.2",
    "autoprefixer": "^9.1.5",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "dom-helpers": "^3.2.0",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "invariant": "^2.2.3",
    "keycode": "^2.1.2",
    "mini-css-extract-plugin": "^0.4.4",
    "net": "^1.0.2",
