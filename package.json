    "prop-types-extra": "^1.1.0",
    "razzle": "^2.4.0",
    "react-context-toolbox": "^1.2.1",
    "react-overlays": "^1.0.0-beta.17",
    "react-prop-types": "^0.4.0",
    "react-transition-group": "^2.4.0",
    "uncontrollable": "^6.0.0",
    "warning": "^4.0.1"
  }
}
